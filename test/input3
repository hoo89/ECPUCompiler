START: LD IX,7H
LOOP: LD ACC,1H
ST IX,(10H)     ;shift IX bit ACC. tmp1 = 1X
LOOP1: SLL ACC
SUB IX,1H
BNZ LOOP1
LD IX,(10H)
AND ACC,(1H)	;1H=y
BNZ SKIP	;if 0 skip else add shifted y to z
LD ACC,(1H)
BA SHIFT2	;IX bit shift y
QSHIFT2: ADC ACC,(2H)
ST ACC,(2H)
LD ACC,(3H)
ADC ACC,0H	;ACC=ACC+CF
ST ACC,(3H)
SKIP: SUB IX,1H
BNZ LOOP
HLT
SHIFT2: ST IX,(10H)
LOOP2: SLL ACC
ST ACC,(11H)
LD ACC,(3H)
ADC ACC,0H	;ACC=ACC+CF
ST ACC,(3H)
LD ACC,(11H)
BNZ LOOP2
LD IX,(10H)
BA QSHIFT2
